{
  "name": "thefront",
  "author": "maccarian agency",
  "email": "hi@maccarianagency.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "GENERATE_SOURCEMAP=false next build",
    "start": "next start",
    "prettier": "prettier --write '**/*.{js,css,html}'",
    "assets": "rimraf ./public/images && cp-cli ./node_modules/thefront-assets/images ./public/images",
    "leaflet-assets": "rimraf ./src/assets/images/leaflet-assets && cp-cli ./node_modules/leaflet/dist/images ./src/assets/images/leaflet-assets",
    "postinstall": "npm run assets && npm run leaflet-assets"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.39",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.2.1",
    "aos": "^2.3.4",
    "clsx": "^1.0.4",
    "history": "^4.9.0",
    "jarallax": "^1.12.1",
    "leaflet": "^1.6.0",
    "next": "9.3.5",
    "next-images": "^1.4.1",
    "node-sass": "^4.14.1",
    "npm": "^6.14.7",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-countup": "^4.3.3",
